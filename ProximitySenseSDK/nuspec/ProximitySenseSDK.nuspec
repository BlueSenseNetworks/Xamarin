        <file src="../ProximitySenseSDK.Droid/bin/Release/ProximitySenseSDK.dll" target="lib/MonoAndroid10/ProximitySenseSDK.dll" />
        <file src="../ProximitySenseSDK.Droid/bin/Release/ProximitySenseSDK.Droid.dll" target="lib/MonoAndroid10/ProximitySenseSDK.Droid.dll" />

