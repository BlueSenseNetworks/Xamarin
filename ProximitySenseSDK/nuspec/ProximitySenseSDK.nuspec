<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>ProximitySenseSDK</id>
        <version>0.0.1</version>
        <title>ProximitySense SDK for Xamarin and Windows</title>
        <description>SDK for ProximitySense - the most advanced proximity cloud platform. Enables mobile apps with contextual proximity technology</description>
        <authors>Vladimir Petrov</authors>
        <owners>Blue Sense Networks ltd</owners>
        <projectUrl>https://github.com/BlueSenseNetworks/Xamarin</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>ibeacon proximitysense sdk windows xamarin xam.pcl</tags>
        <releaseNotes>
0.0.1
Initial release, Xamarin iOS support only
        </releaseNotes>
        <dependencies> 
